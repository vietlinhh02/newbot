# Discord Bot Configuration
DISCORD_TOKEN=
CLIENT_ID=851851433770483762

# Database Configuration - Connection Pooling optimized for 10k servers
DATABASE_URL="postgresql://postgres:admin@localhost:5432/test?schema=public&connection_limit=20&pool_timeout=30"
DIRECT_URL="postgresql://postgres:admin@localhost:5432/test?schema=public"

# Production Database URLs (uncomment for production)
# DATABASE_URL="postgresql://user:pass@host:5432/db?sslmode=require&connection_limit=50&pool_timeout=60"
# DIRECT_URL="postgresql://user:pass@host:5432/db?sslmode=require"

# Environment
NODE_ENV=development

# Bot Configuration
BOT_PREFIX=!
BOT_NAME=DiscordBot
BOT_VERSION=1.0.0

# Scaling Configuration
MAX_GUILDS_PER_SHARD=2500
ENABLE_SHARDING=false
CACHE_TTL=300000
RATE_LIMIT_ENABLED=true

# Performance Settings
ENABLE_PERFORMANCE_MONITORING=true
ENABLE_DATABASE_CACHING=true
BATCH_SIZE=100
MAX_CONCURRENT_COMMANDS=50

# Memory Management
MAX_MEMORY_USAGE=512
GARBAGE_COLLECT_INTERVAL=300000

# Redis Configuration (for distributed caching)
# REDIS_URL="redis://localhost:6379"
# REDIS_PASSWORD=""

# Logging
LOG_LEVEL=info
ENABLE_PERFORMANCE_LOGS=true

# Monitoring & Health Check
HEALTH_CHECK_INTERVAL=30000
METRICS_SAVE_INTERVAL=300000 
